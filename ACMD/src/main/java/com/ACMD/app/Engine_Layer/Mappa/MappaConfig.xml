<!--
  Per inserire un nuovo nodo Ã¨ sufficente aggiungere una voce <position>x,y</position> nel tag node
  dove x e y sono valori interi.

  Per creare una nuova connessione tra due nodi del grafo bisogna inserire:
  - position_start, nodo di partenza da dove parte il collegamento (i valory sono x,y come per position)
  - position_end, nodo di arrivo (i valory sono x,y come per position)
  - direction1, prima direzione
  - direction2, seconda direzione
-->

<mappa>
  <node>
    <position>1,19</position>
    <position>8,13</position>
    <position>16,18</position>
    <position>2,11</position>
    <position>17,2</position>
    <position>2,3</position>
  </node>
  
  <connessione>
    <position_start>1,19</position_start>
    <position_end>8,13</position_end>
    <direction1>nord</direction1>
    <direction2>sud</direction2>
  </connessione>

  <connessione>
    <position_start>2,11</position_start>
    <position_end>8,13</position_end>
    <direction1>est</direction1>
    <direction2>ovest</direction2>
  </connessione>

  <connessione>
    <position_start>16,18</position_start>
    <position_end>8,13</position_end>
    <direction1>est</direction1>
    <direction2>sud</direction2>
  </connessione>

  <connessione>
    <position_start>17,2</position_start>
    <position_end>8,13</position_end>
    <direction1>sud</direction1>
    <direction2>est</direction2>
  </connessione>

  <connessione>
    <position_start>17,2</position_start>
    <position_end>2,3</position_end>
    <direction1>ovest</direction1>
    <direction2>est</direction2>
  </connessione>

  <connessione>
    <position_start>8,13</position_start>
    <position_end>2,3</position_end>
    <direction1>ovest</direction1>
    <direction2>est</direction2>
  </connessione>

   <connessione>
    <position_start>16,18</position_start>
    <position_end>8,13</position_end>
    <direction1>nord</direction1>
    <direction2>sud</direction2>
  </connessione>

   <connessione>
    <position_start>2,3</position_start>
    <position_end>17,2</position_end>
    <direction1>est</direction1>
    <direction2>ovest</direction2>
  </connessione>
</mappa>